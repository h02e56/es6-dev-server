{
  "name": "react-es6",
  "version": "1.0.0",
  "description": "es6 development server",
  "keywords": "es6, browserify, babel, babelify, client, server",
  "scripts": {
    "dev": "npm run babel &&  npm run watch-css & npm run watch-js & NODE_ENV=development nodemon --debug indexes5.js ",
    "pro": "npm run babel && NODE_ENV=production DEBUG=* nodemon indexes5.js",
    "watch-js": "watchify ./lib/client/app.js -o ./lib/server/static/js/build.js -d",
    "watch-css": "compass watch",
    "browserify-test": "browserify test/client/test.js > test/client/test.browser.js",
    "babel-test": "babel ./test/server/test.js > ./test/server/serveres5.js",
    "babel": "babel --sourceMaps=both  ./index.js > ./indexes5.js",
    "test-client": "npm run browserify-test && faucet ./test/client/test.browser.js",
    "test-server": "npm run babel-test && faucet ./test/server/serveres5.js ",
    "test": "standard lib && npm run test-server "
  },
  "author": "h02e56",
  "main": "indexes5.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/h02e56/es6-react.git"
  },
  "browserify": {
    "transform": [
      "babelify",
      "reactify"
    ]
  },
  "engine": {
    "node": "^2.3.0"
  },
  "standard": {
    "parser": "babel-eslint"
  },
   "dependencies": {
    "JSONStream": "0.10.0",
    "handlebars-stream": "^1.4.0",
    "react": "^0.13.3",
    "through2": "^2.0.0",
    "ecstatic": "~0.5.8",
    "he": "^0.5.0",
    "routes": "^2.0.0",
    "highland": "2.5.1"
  },
  "devDependencies": {
    "babel": "^5.6.23",
    "babel-eslint": "^3.1.23",
    "babelify": "^6.1.2",
    "browser-sync": "^2.7.13",
    "browserify": "^10.2.4",
    "debug": "^2.2.0",
    "faucet": "0.0.1",
    "node-inspector": "^0.10.2",
    "nodemon": "1.3.7",
    "reactify": "^1.1.1",
    "request": "2.27.0",
    "sinon": "1.15.4",
    "standard": "4.5.4",
    "tap": "^1.0.5",
    "tape": "~2.3.2",
    "watchify": "^3.2.2"
  }
}
